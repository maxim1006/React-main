{
    "name": "react-main",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "proxy": "http://localhost:3001",
    "scripts": {
        "report": "source-map-explorer 'build/static/js/*.js'",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "nodemon -r esm ./server/server.js",
        "build:server": "webpack-dev-server --open --entry /webpack-dev-server --output-path /dist --compress --content-base ./build --history-api-fallback",
        "build:serve": "cd ./build && ws --spa index.html --compress --open --static.maxage -1",
        "deploy": "gh-pages -d build",
        "prelog": "NODE_ENV=prelog node ./logs.js",
        "log": "node ./logs.js",
        "postlog": "NODE_ENV=postlog node ./logs.js",
        "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx ./src/",
        "lint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx ./src/ --fix",
        "precommit": "lint-staged"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier — double-quote — write",
            "git add"
        ]
    },
    "dependencies": {
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.2",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-redux": "^7.1.7",
        "@types/styled-components": "^4.4.1",
        "axios": "^0.19.0",
        "bowser": "^2.8.1",
        "firebase": "^7.6.0",
        "install": "^0.13.0",
        "lodash": "^4.17.15",
        "react": "^16.11.0",
        "react-countup": "^4.3.3",
        "react-dom": "^16.11.0",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.1",
        "react-stripe-checkout": "^2.6.3",
        "react-transition-group": "^4.3.0",
        "redux": "^4.0.4",
        "redux-form": "^8.2.6",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^4.4.1",
        "typescript": "^3.8.3"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "babel-eslint": "10.1.0",
        "babel-plugin-styled-components": "^1.10.6",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-airbnb-typescript": "^7.2.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.2",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "faker": "^4.1.0",
        "fs-extra": "^8.1.0",
        "gh-pages": "^2.1.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "node-sass": "^4.13.0",
        "nodemon": "^1.19.4",
        "npm": "^6.13.0",
        "prettier": "^1.19.1",
        "redux-devtools-extension": "^2.13.8",
        "source-map-explorer": "^1.8.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "3.10.3"
    }
}
