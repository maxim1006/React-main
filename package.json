{
    "name": "react-main",
    "version": "0.1.0",
    "engines": {
        "node": "13.7.0",
        "npm": "6.13.1"
    },
    "private": true,
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon -r esm ./server/server.js",
        "server:prod": "NODE_ENV=production nodemon -r esm ./server/server.js",
        "build": "cd ./client && npm run build && cd ../",
        "start": "node -r esm ./server/server.js",
        "heroku-postbuild": "cd ./client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
    },
    "dependencies": {
        "@types/jest": "^25.2.1",
        "@types/node": "^13.11.0",
        "@types/react": "^16.9.32",
        "@types/react-dom": "^16.9.6",
        "apollo-server": "^2.11.0",
        "apollo-server-express": "^2.11.0",
        "body-parser": "^1.18.3",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "7.0.0",
        "esm": "^3.2.25",
        "express": "^4.16.4",
        "faker": "^4.1.0",
        "fs-extra": "^8.1.0",
        "graphql": "^14.6.0",
        "mongoose": "^5.9.5",
        "node-fetch": "^2.6.0",
        "raw-body": "^2.4.1",
        "stripe": "6.28.0",
        "typescript": "^3.8.3"
    }
}
